    #include<bits/stdc++.h>
    using namespace std;
    int main()
    {
        int n,k;
        cin>>n>>k;
        while(k!=0)
        {
          int ld=n%10;
          if(ld!=0)
          {
              n-=1;
          }
          else
          {
            n/=10;
          }
          k--;
     
        }
        cout<<n<<endl;
        return 0;
    }
